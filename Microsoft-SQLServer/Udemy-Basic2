
【テーブルの作成】
create table SampleTable(
id int identity(1,1) not null,
Name varchar(20) not null,
quantity int not null
primary key(
Id)
)

【単一データの追加】
insert into SampleTable(Name,quantity) Values('Banana',1)
<結果表示>
Select * from SampleTable

Insert into SampleTable Values ('Apple',2)

【複数データの追加】
insert into SampleTable(Name, quantity)
Values ('Orange',3)
,('Lemon',4)
,('Meron',2)

<結果表示>
Select * from SampleTable

【テーブルのコピー】
Select * into SampleTableCopy77 from sampletable
Select * from SampleTableCopy77

【テーブルの一部の条件のみコピー】
select * into sampletablecopy22 from
(select * from sampletable where name='Banana') as table1

Select * from SampleTableCopy22

【空のテーブルのコピー】
select * into sampletablecopy33
from (select * from sampletable where 0=1) as t1

Select * from SampleTableCopy33

【テーブルから別のテーブルへの値のinsert】
insert into [dbo].[sampletablecopy33]
select name,quantity  from [dbo].[SampleTable]]

